{
  "name": "@qawolf/ci-utils",
  "version": "1.1.0",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^16 || ^18 || ^20 || >=22"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --splitting --treeshake --sourcemap --outDir dist --target es2022",
    "lint": "cycle-import-scan . && eslint . --ext js,jsx,mjs,ts,tsx --quiet && prettier --check .",
    "lint:fix": "eslint . --ext js,jsx,mjs,ts,tsx --fix --quiet && prettier --log-level=warn --write .",
    "lint:warnings": "eslint . --ext js,jsx,mjs,ts,tsx",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "tsc:check": "tsc",
    "prepublishOnly": "nx run ci-utils:build",
    "gen": "npm run build"
  },
  "devDependencies": {
    "tsup": "8.3.5"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "tslib": "^2.5.3",
    "zod": "^3.23.8"
  },
  "license": "MIT",
  "files": [
    "/dist",
    "/README.md"
  ]
}
